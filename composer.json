{
    "name": "masum/laravel-tagging",
    "description": "A comprehensive Laravel package for automatic tag generation and management with barcode support, events, bulk operations, and performance optimizations",
    "keywords": [
        "laravel",
        "tagging",
        "tags",
        "auto-generate",
        "polymorphic",
        "barcode",
        "label-printing",
        "qr-code",
        "sequential-tags",
        "inventory",
        "asset-management",
        "events",
        "caching"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Masum Nishat",
            "email": "masum@example.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/MasumNishat/laravel-tagging",
    "support": {
        "issues": "https://github.com/MasumNishat/laravel-tagging/issues",
        "source": "https://github.com/MasumNishat/laravel-tagging",
        "docs": "https://github.com/MasumNishat/laravel-tagging#readme"
    },
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "picqer/php-barcode-generator": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Masum\\Tagging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masum\\Tagging\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masum\\Tagging\\TaggingServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-unit": "vendor/bin/phpunit --testsuite Unit",
        "test-feature": "vendor/bin/phpunit --testsuite Feature"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}